{
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    }
  ],
  "crons": [
    {
      "path": "/api/push/send?action=scheduled&timeOfDay=MORNING",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/push/send?action=scheduled&timeOfDay=LUNCH",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/push/send?action=scheduled&timeOfDay=DINNER",
      "schedule": "0 18 * * *"
    },
    {
      "path": "/api/push/send?action=scheduled&timeOfDay=BEFORE_SLEEP",
      "schedule": "0 22 * * *"
    }
  ]
}
